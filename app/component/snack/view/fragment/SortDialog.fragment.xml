<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    >
    <ViewSettingsDialog
        id="SortDialog"
        confirm="onConfirmSortDialog"
        title="간식 목록 정렬">
        <sortItems>
            <ViewSettingsItem text="간식 카테고리" key="snack_category" selected="true"/>
            <ViewSettingsItem text="간식 코드" key="snack_code" selected="true"/>
            <ViewSettingsItem text="간식 이름" key="snack_name"/>
            <ViewSettingsItem text="간식 가격" key="snack_price"/>
            <ViewSettingsItem text="재고" key="snack_stock"/>
            <ViewSettingsItem text="재고 상태" key="snack_status"/>
            <ViewSettingsItem text="재고 신청일" key="snack_date"/>
            <ViewSettingsItem text="선호도" key="snack_prefer"/>

        </sortItems>
    </ViewSettingsDialog>
</core:FragmentDefinition>